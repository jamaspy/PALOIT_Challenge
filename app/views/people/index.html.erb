<h1>All People</h1>
<%= link_to("Add Name", "/people/new") %>
<table>
<tr>
    <th>Firstname</th>
    <th>Lastname</th> 
    <th>Ascii</th>
    <th>Binary</th>
    <th>Zeros</th>
  </tr>

<% @people.each do |person|%>
<tr>
    <td><%=person.firstname%></td> 
    <td><%=person.lastname%></td>
    <td><%=person.asciify%></td>
    <td><%=person.binarify%></td>
    <td><%=person.countZeros%></td>
</tr>
<% end %>
</table>